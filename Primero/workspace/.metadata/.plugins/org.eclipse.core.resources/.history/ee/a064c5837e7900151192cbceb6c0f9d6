package estructurada;

import java.util.Scanner;

public class Blackjack2 {

	public static void main(String[] args) {

		

	}
	public static int apostar(int apuesta) {
		int apuesta1 = 50, apuesta2 = 50;
		int apostado = 0;
		int dinero1 = 100, dinero2 = 100;
		dinero1 -= apuesta;
		dinero2 -= apuesta;
		return apostado;
		
	}

}
