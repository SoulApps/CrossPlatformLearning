package actions;

import banco.Cuenta;

public class IngresarDineroAccion implements Accion {

		Cuenta cuenta;
		double ingreso;
		
		public IngresarDineroAccion(Cuenta cuenta,double ingreso){
			this.cuenta=cuenta;
			this.ingreso=ingreso;
		}

		public Object execute() {
			cuenta.setSaldo(cuenta.getSaldo()+ingreso);
			return cuenta;
		}
		
}
